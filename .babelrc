  "ignore": ["src/**/__specs__"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-transform-modules-commonjs",
    ["@babel/plugin-proposal-private-methods", { "loose": true }]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ]
}
